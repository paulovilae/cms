    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "payload",
    "plugin",
    "core",
    "orchestrator",
    "multi-tenant",
    "business-context",
    "plugin-management"
  ],
  "author": "Paulo Vila",
  "license": "MIT",
  "peerDependencies": {
    "payload": "^2.0.0 || ^3.0.0",
    "next": "^13.0.0 || ^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "payload": {
    "type": "plugin",
    "category": "core",
