{
  "name": "@paulovila/core-orchestrator",
  "version": "1.0.0",
  "description": "Core orchestrator plugin for multi-tenant business context detection and plugin management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
